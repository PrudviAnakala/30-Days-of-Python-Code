      "source": [
        "import sys\n",
        "n = int(input())\n",
        "result =0\n",
        "maximum=0\n",
        "while n>0:\n",
        "    if n%2==1:\n",
        "        result=result+1\n",
        "        if result > maximum:\n",
        "            maximum =result \n",
        "    else:\n",
        "        result=0\n",
        "    n=n//2\n",
        "print(maximum)"
      ],
